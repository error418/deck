{
  "name": "@swingletree-oss/deck",
  "description": "Swingletree webinterface",
  "repository": {
    "type": "git",
    "url": "git@github.com:swingletree-oss/deck.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run build && node bin/main.js",
    "sass": "node-sass views/sass/main.scss -o static/css --output-style compressed",
    "sass:watch": "npm run sass -- -w",
    "build": "npm run sass && npm run build-ts && npm run tslint",
    "test": "ts-mocha --exit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --inspect bin/main.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.1",
    "@swingletree-oss/harness": "^1.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "d3": "^5.15.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "inversify": "^5.0.1",
    "jdenticon": "^2.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nconf": "^0.10.0",
    "node-cache": "^5.1.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "@types/basic-auth": "^1.1.3",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-things": "0.0.34",
    "@types/compression": "1.7.0",
    "@types/express": "^4.17.4",
    "@types/express-jwt": "0.0.42",
    "@types/js-yaml": "^3.12.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "^7.0.2",
    "@types/nconf": "^0.10.0",
    "@types/node": "^13.11.0",
    "@types/node-cache": "^4.2.5",
    "@types/nunjucks": "^3.1.3",
    "@types/passport": "^1.0.3",
    "@types/redis": "^2.8.17",
    "@types/request": "^2.48.4",
    "@types/sinon": "^7.5.2",
    "bootstrap-scss": "^4.4.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.1",
    "mockserver": "^3.1.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "semantic-release": "^17.0.4",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.5.0",
    "sinon-express-mock": "^2.2.1",
    "ts-mocha": "^6.0.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "license": "GPL-3.0",
  "private": true
}
